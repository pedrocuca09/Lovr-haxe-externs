package lovr.math;

/**
 * Represents a quaternion.
 * https://lovr.org/docs/Quat
 */
extern class Quat {
  /**
   * Gets the x component.
   */
  public var x:Float;

  /**
   * Gets the y component.
   */
  public var y:Float;

  /**
   * Gets the z component.
   */
  public var z:Float;

  /**
   * Gets the w component.
   */
  public var w:Float;

     
} 