package lovr.math;

/**
 * Represents a 4D vector.
 * https://lovr.org/docs/Vec4
 */
extern class Vec4 {
  /**
   * Gets the x component.
   */
  public var x:Float;

  /**
   * Gets the y component.
   */
  public var y:Float;

  /**
   * Gets the z component.
   */
  public var z:Float;

  /**
   * Gets the w component.
   */
  public var w:Float;

     
} 