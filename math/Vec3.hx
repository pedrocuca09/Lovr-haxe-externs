package lovr.math;

/**
 * Represents a 3D vector.
 * https://lovr.org/docs/Vec3
 */
extern class Vec3 {
  /**
   * Gets the x component.
   */
  public var x:Float;

  /**
   * Gets the y component.
   */
  public var y:Float;

  /**
   * Gets the z component.
   */
  public var z:Float;

     
} 